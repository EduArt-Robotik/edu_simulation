      <!-- Joints. -->
      <joint type="revolute" name="axis_front_left">
        <pose>0 0 0 0 0 0</pose>
        <child>wheel_front_left</child>
        <parent>chassis</parent>
        <axis>
          <xyz>0 0 1</xyz>
        </axis>
      </joint>
