      <!-- LIDAR Sensor -->
      <link name="lidar"> 
        <pose>0.115 0 0.12 0 0 0</pose>   
        <inertial>
          <inertia>
            <ixx>3.0e-6</ixx>
            <iyy>3.0e-6</iyy>
